#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
int n,i,j,v;
cout<<"no. of vertices"<<endl;
cin>>n;
int a[n][n];
cout<<"input the adjacency matrix"<<endl;
for(i=0;i<n;i++)
for(j=0;j<n;j++)
cin>>a[i][j];
cout<<"input the vertex"<<endl;
cin>>v;
i=v;
for(j=0;j<n;j++)
{
if(a[i][j]==1)
cout<<j<<" ";
}
}
